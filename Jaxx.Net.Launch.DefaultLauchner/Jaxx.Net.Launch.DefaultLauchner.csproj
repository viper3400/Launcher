    <UseWPF>true</UseWPF>
    <AssemblyName>Jaxx.Net.Launch.DefaultLauchner</AssemblyName>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="config.txt" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Prism.Wpf" Version="8.0.0.1909" />
  </ItemGroup>
